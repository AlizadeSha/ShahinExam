
@{
    ViewData["Title"] = "Update";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}


@{
    ViewData["Title"] = "Create";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}
@model UpdateEmployeeVM
<div class="card-body">
    <form method="post" enctype="multipart/form-data">
        <div class="mb-3">
            <label asp-for="Name" class="form-label">Name</label>
            <input asp-for="Name" class="form-control">
            <span asp-validation-for="Name"></span>
        </div>
        <div class="mb-3">
            <label asp-for="Position" class="form-label">Position </label>
            <input asp-for="Position" class="form-control">
            <span asp-validation-for="Position"></span>
        </div>
        <div class="mb-3">
            <label asp-for="Description" class="form-label">Description </label>
            <input asp-for="Description" class="form-control">
            <span asp-validation-for="Description"></span>
        </div>
        <div class="mb-3">
            <label asp-for="ImageUrl" class="form-label">Image Url</label>
            <input asp-for="ImageUrl" class="form-control">
            <span asp-validation-for="ImageUrl"></span>
        </div>
        <div class="mb-3">
            <label asp-for="Twitter" class="form-label">Twitter</label>
            <input asp-for="Twitter" class="form-control">
            <span asp-validation-for="Twitter"></span>
        </div>
        <div class="mb-3">
            <label asp-for="Facebook" class="form-label"> Facebook</label>
            <input asp-for="Facebook" class="form-control">
            <span asp-validation-for="Facebook"></span>
        </div>
        <div class="mb-3">
            <label asp-for="Instagram" class="form-label"> Instagram</label>
            <input asp-for="Instagram" class="form-control">
            <span asp-validation-for="Instagram"></span>
        </div>
        <div class="mb-3">
            <label asp-for="LinkedIn" class="form-label"> LinkedIn</label>
            <input asp-for="LinkedIn" class="form-control">
            <span asp-validation-for="LinkedIn"></span>
        </div>
        <button type="submit"  class="btn btn-primary">Submit</button>
    </form>
</div>


